{
    "manifest_version": 3,
    "name": "Auto Refresh",
    "version": "1.0",
    "description": "Automatically refreshes the current page at regular intervals.",
    "permissions": ["tabs","storage"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  